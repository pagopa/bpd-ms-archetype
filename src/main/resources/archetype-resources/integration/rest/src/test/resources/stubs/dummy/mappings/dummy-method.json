{
  "request": {
    "method": "GET",
    "urlPathPattern": "/dummy/dummy-method/(\\w*)"
  },
  "response": {
    "status": "200",
    "jsonBody": {
      "id": "{{request.path.[2]}}"
    },
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": [
      "response-template"
    ]
  }
}
